/*animacion*/
@keyframes removed-item-animation {
  0% {
    opacity: 1;
    -webkit-transform: rotateZ(0);
    -ms-transform: rotateZ(0);
    -o-transform: rotateZ(0);
    transform: rotateZ(0);
  }
  100% {
    opacity: 0;
    -webkit-transform: translateY(600px) rotateZ(90deg);
    -ms-transform: translateY(600px) rotateZ(90deg);
    -o-transform: translateY(600px) rotateZ(90deg);
    transform: translateY(600px) rotateZ(90deg);
  }
}
@-webkit-keyframes removed-item-animation {
  0% {
    opacity: 1;
    -webkit-transform: rotateZ(0);
    transform: rotateZ(0);
  }
  100% {
    opacity: 0;
    -webkit-transform: translateY(600px) rotateZ(90deg);
    transform: translateY(600px) rotateZ(90deg);
  }
}
@-o-keyframes removed-item-animation {
  0% {
    opacity: 1;
    -o-transform: rotateZ(0);
    transform: rotateZ(0);
  }
  100% {
    opacity: 0;
    -o-transform: translateY(600px) rotateZ(90deg);
    transform: translateY(600px) rotateZ(90deg);
  }
}
/*postit */
@font-face {
  font-family: AlwaysForever;
  src: url('../phpfont/alwaysforever.ttf');
}
.container-postit ul * {
  margin: 0;
  padding: 0;
}
.container-postit h2,
.container-postit p {
  font-size: 100%;
  font-weight: normal;
}
.container-postit ul,
.container-postit li {
  list-style: none;
}
.container-postit ul {
  overflow: hidden;
  padding: 1.5em;
  width : 100%;
}
.container-postit ul li a &gt; p {
  overflow-y: scroll;
  height: 80%;
}
.container-postit ul li a {
  cursor: pointer;
  text-decoration: none;
  color: #000;
  background: #ffc;
  display: block;
  height: 19em;
  width: 16em;
  padding: 1em;
  -moz-box-shadow: 5px 5px 7px #212121;
  -webkit-box-shadow: 5px 5px 7px rgba(33, 33, 33, 0.7);
  box-shadow: 5px 5px 7px rgba(33, 33, 33, 0.7);
  -moz-transition: -moz-transform 0.15s linear;
  -o-transition: -o-transform 0.15s linear;
  -webkit-transition: -webkit-transform 0.15s linear;
}
.container-postit ul li {
  margin: 2.3em 1em;
  float: left;
}
.container-postit ul li h2 {
  font-size: 140%;
  font-weight: bold;
  padding-bottom: 10px;
}
.container-postit ul li p {
  font-family: AlwaysForever;
  font-weight: bold;
  font-size: 190%;
  color: #666;
}
.container-postit ul li a {
  -webkit-transform: rotate(-6deg);
  -o-transform: rotate(-6deg);
  -moz-transform: rotate(-6deg);
}
.container-postit ul li:nth-child(even) a {
  -o-transform: rotate(4deg);
  -webkit-transform: rotate(4deg);
  -moz-transform: rotate(4deg);
  position: relative;
  top: 5px;
  background: #cfc;
}
.container-postit ul li:nth-child(3n) a {
  -o-transform: rotate(-3deg);
  -webkit-transform: rotate(-3deg);
  -moz-transform: rotate(-3deg);
  position: relative;
  top: -5px;
  background: #ccf;
}
.container-postit ul li:nth-child(5n) a {
  -o-transform: rotate(5deg);
  -webkit-transform: rotate(5deg);
  -moz-transform: rotate(5deg);
  position: relative;
  top: -10px;
}
.container-postit ul li a:hover,
.container-postit ul li a:focus {
  box-shadow: 10px 10px 7px rgba(0, 0, 0, 0.7);
  -moz-box-shadow: 10px 10px 7px rgba(0, 0, 0, 0.7);
  -webkit-box-shadow: 10px 10px 7px rgba(0, 0, 0, 0.7);
  -webkit-transform: scale(1.25);
  -moz-transform: scale(1.25);
  -o-transform: scale(1.25);
  position: relative;
  z-index: 5;
}
.container-postit li div.tape {
  position: absolute;
  left: 60px;
  top: -15px;
  width: 75px;
  height: 25px;
  z-index: 2;
  background-color: rgba(243, 245, 228, 0.5);
  border: 2px solid rgba(255, 255, 255, 0.5);
  -webkit-box-shadow: 0 0 5px #888;
  -moz-box-shadow: 0 0 5px #888;
  box-shadow: 2px 2px 2px #000;
}
.container-postit ul li.drop {
  -webkit-animation: removed-item-animation 1s cubic-bezier(0.55, -0.04, 0.91, 0.94) forwards;
  -o-animation: removed-item-animation 1s cubic-bezier(0.55, -0.04, 0.91, 0.94) forwards;
  animation: removed-item-animation 1s cubic-bezier(0.55, -0.04, 0.91, 0.94) forwards;
  -webkit-transform-origin: 0% 100%;
  -ms-transform-origin: 0% 100%;
  -o-transform-origin: 0% 100%;
  transform-origin: 0% 100%;
}
