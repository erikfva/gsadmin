<CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Inserted" ScriptCtrlID="" ScriptL1Key="" ScriptL2Key="">
<![CDATA[// Row Inserted event
function Row_Inserted($rsold, &$rsnew) {
	//echo "Row Inserted"

	//actualizando el nombre del propietario del token de antecedentes
	$LS_db = connLimeSurvey();
	$LimeSurvey = $_SESSION["LimeSurvey"];
	$firstname = $rsnew["nombres"] . ($rsnew["appaterno"]?" ".$rsnew["appaterno"]:"") . ($rsnew["apmaterno"]?" ".$rsnew["apmaterno"]:"") ;
	$SQL = "UPDATE lime_tokens_".$LimeSurvey["antecedentesID"]." SET firstname = '".$firstname."', validuntil = NULL WHERE token = '".$rsnew["token_antecedentes"]."'";
	$ls = $LS_db->Execute($SQL);  
	
	if( !empty($LimeSurvey["registroID"])){    
		//actualizando el nombre del propietario del token de registro
		$SQL = "UPDATE lime_tokens_".$LimeSurvey["registroID"]." SET firstname = '".$firstname."', validuntil = NULL WHERE `token` = '".$rsnew["token_registro"]."'";
		$ls = $LS_db->Execute($SQL);  
	}
	$LS_db->Close();
	
	global $_SESSION;
	$_SESSION["mk_consulta_numeroexpediente"] = $rsnew["numeroexpediente"];   


	$opciones = $_SESSION[CurrentPage()->PageObjName."_opciones"];

	if (AllowAdd(CurrentProjectID() . "consulta")){
		$addConsulta = (comportamiento("100") || (!empty($opciones) && strpos($opciones,"addconsulta")>-1) ) && strpos($this->getCurrentDetailTable(),"cita_medica")===false && strpos($this->getCurrentDetailTable(),"consulta")===false;
		if(!empty($opciones) && strpos($opciones,"addconsulta")>-1){
			$_SESSION[CurrentPage()->PageObjName."_opciones"] = str_replace("addconsulta","",$opciones);
		}
	
		if($addConsulta){ //agregar primera consulta automaticamente
			if(CurrentUserInfo("idmedico")){
				$SQL = "INSERT INTO consulta(fecha, numeroexpediente, idmedico) VALUES ('".date('Y-m-d H:i:s', time())."','".$rsnew["numeroexpediente"]."','".CurrentUserInfo("idmedico")."')";
				$addsuccess = ew_Execute($SQL);
				$idconsulta = $addsuccess? $conn->Insert_ID(): "";
				$_SESSION["pacientelist_run_script"] = $idconsulta?"top.consultaedit('" .$idconsulta."')":"" ;
			}else{
				$_SESSION["pacientelist_run_script"] = "top.consultaadd('" .$rsnew["numeroexpediente"]."')";
			}
		} else { //preguntar si desea agregar la primera consulta
				$htmlDialog = '
		<div class="modal fade">
	  <div class="modal-dialog modal-sm">
		<div class="modal-content">
		  <div class="modal-body">
		  	<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
			<p>Desea crear una consulta ahora?</p>
		  </div>
		  <div class="modal-footer">
			<button type="button" class="btn btn-danger" data-dismiss="modal">No</button>
			<button type="button" class="btn btn-success" data-dismiss="modal" onclick="%s">Si</button>
		  </div>
		</div><!-- /.modal-content -->
	  </div><!-- /.modal-dialog -->
	</div>
	';
				$_SESSION["pacientelist_run_script"] = "$('".str_replace("%s","top.consultaadd(\'" .$rsnew["numeroexpediente"]."\')",ew_JsEncode2(trim($htmlDialog)))."').appendTo('body').modal('show'); "; 
		}
	}

	if(strpos($this->getCurrentDetailTable(),"consulta")!==false && CurrentUserInfo("idmedico")){
		$idconsulta = ew_ExecuteScalar("SELECT idconsulta FROM consulta WHERE numeroexpediente=".$rsnew["numeroexpediente"]);
		$_SESSION["pacientelist_run_script"] = $idconsulta?"top.consultaedit('" .$idconsulta."')":"" ;
	}
}
]]>
</CustomScript>
