<![CDATA[// Recordset Selected event
function Recordset_Selected(&$rs) {
	//echo "Recordset Selected"; 
	global $plantillas;
	$dir = 'busqueda/templates';

	// Abrir un directorio conocido, y proceder a leer sus contenidos
	if (is_dir($dir)) {
		if ($gd = opendir($dir)) {
			while ($archivo = readdir($gd)) {
			// comprobamos que sean archivos y no otras carpetas
				if(filetype($dir .'/'. $archivo)!="dir"){
					$info = pathinfo($archivo);
					$file_name =  basename($archivo,'.'.$info['extension']);
					$plantillas[$file_name] = file_get_contents($dir .'/'. $archivo); //Leemos y guardamos
//  echo "nombre de archivo: ".$file_name." : tipo de archivo: " . filetype($dir . $archivo) . "\n";
}      
}
closedir($gd);        
}
}  
}
